<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <TargetFrameworks>net10.0;net7.0;net9.0;netstandard2.0;netstandard2.1</TargetFrameworks>
        <Version>2.0.0-alpha.6</Version>
        <LangVersion>14.0</LangVersion>
        <Title>TrajectoryLogReader.DICOM</Title>
        <Description>Provides methods for calculating fluence using DICOM plan data</Description>
    </PropertyGroup>

    <PropertyGroup>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="fo-dicom.NetCore" Version="4.0.8" />
      <PackageReference Include="System.Data.SqlClient" Version="4.8.6" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\TrajectoryLogReader\TrajectoryLogReader.csproj" />
    </ItemGroup>

</Project>
